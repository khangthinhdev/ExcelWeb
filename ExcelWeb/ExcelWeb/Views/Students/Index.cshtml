@model IEnumerable<Student>
@{
    Layout = null;
}

<form enctype="multipart/form-data" asp-controller="Students" asp-action="Index" method="post">
    <input type="file" name="file"/>
    <hr />
    <button type="submit">Upload</button>
</form>

@if (Model.Count() > 0)
{
    <hr />
    <table cellpadding="0" cellspacing="0" border="1">
        <tr>
            <td>Name</td>
            <td>Age</td>
            <td>Email</td>
        </tr>
        @foreach (Student student in Model)
        {
            <tr>
                <td>@student.Name</td>
                <td>@student.Age</td>
                <td>@student.Email</td>
            </tr>
        }
    </table>
}
